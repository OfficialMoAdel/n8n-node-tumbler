{
    "name": "n8n-nodes-tumblr",
    "version": "0.1.0",
    "description": "n8n node for Tumblr integration",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "https://github.com/OfficialMoAdel/n8n-nodes-tumblr#readme",
    "author": {
        "name": "Mohamed",
        "email": "Mohamed.adel.3972@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/OfficialMoAdel/n8n-nodes-tumblr.git"
    },
    "bugs": {
        "url": "https://github.com/OfficialMoAdel/n8n-nodes-tumblr/issues"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials package.json",
        "lintfix": "eslint nodes credentials package.json --fix",
        "prepublishOnly": "npm run build && npm run lint -s",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "clean": "rimraf dist coverage",
        "prepack": "npm run clean && npm run build",
        "validate": "npm run lint && npm run test && npm run build"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/TumblrOAuth2Api.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Tumblr/Tumblr.node.js"
        ]
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-n8n-nodes-base": "^1.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "gulp": "^4.0.2",
        "jest": "^29.5.0",
        "n8n-workflow": "^1.82.0",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.0",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=20.15.0",
        "npm": ">=10.0.0"
    },
    "dependencies": {
        "tumblr.js": "5.0.1"
    },
    "peerDependencies": {
        "n8n-workflow": ">=1.0.0"
    }
}